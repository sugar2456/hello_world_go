{
    "name": "GO Development Container",
    "dockerComposeFile": ["../compose.yml"],
    "service": "app",
    "workspaceFolder": "/usr/src/app",
    "shutdownAction": "none",
    "remoteUser": "vscode",
    "updateRemoteUserUID": true,
    
    // その他の設定...
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.go"
            ]
        }
    }
}